      "query": {
        "collPath": "projects/projectID/databases/(default)/documents/C",
        "clauses": [
          {
            "orderBy": {
              "path": {
                "field": [
                  "a"
                ]
              },
              "direction": "asc"
            }
          },
          {
            "startAt": {}
          }
        ],
        "isError": true
      }
    }
  ]
}
